{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "paths": { "~/*": ["src/*"] },
    "skipLibCheck": true,
    "target": "esnext",
    "resolveJsonModule": true
  },
  "exclude": [
    "env.development.ts"
  ]
}
